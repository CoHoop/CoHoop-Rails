<% present @feed, presenters: [:user, :feed] do |presenter| %>


  <div id="leftSidebar" class="well">

    <div>
      <h2>Show</h2>
      <%= presenter.filters %>
    </div>

  </div>


  <div id="feedContent">

    <div class="tabs">
      <%= presenter.feed_type %>
    </div>

    <div class="postMicrohoop">
      <h3>Post a microhoop</h3>
      <%= presenter.add_microhoop_form %>
    </div>

    <div class="listMicrohoop">
      <h2>Community Feed</h2>
      <%= presenter.feed_content %>
    </div>

  </div>


<% end %>
